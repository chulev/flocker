{
  "name": "flocker",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "biome check --diagnostic-level=error",
    "lint:fix": "biome check --write",
    "db:migrate": "bun run src/scripts/manage-db.ts lib/db/migrations",
    "schema:generate": "kysely-codegen --dialect postgres --out-file src/lib/db/schema.ts",
    "analyze": "ANALYZE=true next build",
    "type:check": "tsc",
    "format:check": "biome format",
    "format:fix": "biome format --write"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.9.0",
    "@redis/client": "^1.5.14",
    "class-variance-authority": "^0.7.0",
    "kysely": "^0.27.2",
    "kysely-neon": "^1.3.0",
    "next": "16.1.1",
    "next-auth": "^5.0.0-beta.30",
    "nextjs-toploader": "^1.6.6",
    "pg": "^8.11.3",
    "react": "^18",
    "react-dom": "^18",
    "sharp": "0.32.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@happy-dom/global-registrator": "^14.3.8",
    "@next/bundle-analyzer": "^14.1.0",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@types/bun": "^1.0.10",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/pg": "^8.11.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "kysely-codegen": "^0.11.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "tw-colors": "^3.3.1",
    "typescript": "^5"
  }
}
